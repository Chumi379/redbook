<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.chumi.redbook.auth.domain.mapper.UserDOMapper">
    <resultMap id="BaseResultMap" type="top.chumi.redbook.auth.domain.dataobject.UserDO">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="username" property="username" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
    </resultMap>
    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select *
        from t_user
        where id = #{id}
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
        delete
        from t_user
        where id = #{id}
    </delete>

    <insert id="insert" parameterType="top.chumi.redbook.auth.domain.dataobject.UserDO">
        insert into t_user (username, create_time, update_time)
        values (#{username}, #{createTime}, #{updateTime})
    </insert>

    <update id="updateByPrimaryKey" parameterType="top.chumi.redbook.auth.domain.dataobject.UserDO">
        update t_user
        set username    = #{username},
            create_time = #{createTime},
            update_time = #{updateTime}
        where id = #{id}
    </update>
</mapper>
